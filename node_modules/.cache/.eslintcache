d) {\n            removeLikes(id)\n            userLiked = !userLiked\n        } else {\n            addLikes(id)\n            userLiked = !userLiked\n        }    \n    }\n\n    return (\n        <div className=\"ui card\" style={tempCardStyle}>\n            <i onClick={handleLike} className={userLiked? \"heart icon\" : \" heart outline icon\"}></i>\n            <img src={image} className=\"ui small centered image\" style={imgStyle}/>\n            <br></br>\n            <div >\n            <h3 className=\"ui header\">{title}</h3>\n            <h5 className=\"ui header\">{seller}</h5>\n            <h5 className=\"ui header\">${price}</h5> \n            <button disabled={inStock ? '' : \"disabled\"} className=\"ui button\" onClick={handleCartToggle}>{inStock ? ((cart.includes(id))? \"Remove from Cart\" : \"Add to Cart\") : \"Out of Stock\"}</button>\n            </div>\n        </div>\n    )\n}\n\nexport default ProductCard\n","/Users/vinceboogie/Development/DunderList/src/Data/shopProducts.js",[],{"ruleId":"99","severity":1,"message":"100","line":32,"column":5,"nodeType":"101","endLine":32,"endColumn":7,"suggestions":"102"},{"ruleId":"99","severity":1,"message":"103","line":69,"column":5,"nodeType":"101","endLine":69,"endColumn":15,"suggestions":"104"},{"ruleId":"99","severity":1,"message":"105","line":75,"column":5,"nodeType":"101","endLine":75,"endColumn":15,"suggestions":"106"},{"ruleId":"107","severity":1,"message":"108","line":182,"column":39,"nodeType":"109","messageId":"110","endLine":182,"endColumn":41},{"ruleId":"111","severity":1,"message":"112","line":184,"column":9,"nodeType":"113","messageId":"114","endLine":184,"endColumn":42},{"ruleId":"115","severity":1,"message":"116","line":145,"column":49,"nodeType":"117","endLine":145,"endColumn":116},{"ruleId":"115","severity":1,"message":"116","line":146,"column":49,"nodeType":"117","endLine":146,"endColumn":116},{"ruleId":"115","severity":1,"message":"116","line":147,"column":49,"nodeType":"117","endLine":147,